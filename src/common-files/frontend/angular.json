{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "src/app/view",
  "projects": {
    "web": {
      "projectType": "library",
      "root": "src/app/view/web",
      "sourceRoot": "src/app/view/web",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "src/app/view/web/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "tsconfig.build.json"
            },
            "development": {
              "tsConfig": "tsconfig.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "angular.test-setup-file.ts",
            "tsConfig": "./tsconfig.spec.json",
            "karmaConfig": "./karma.conf.js",
            "include": [
              "src/**/*.?(c-|i-)test.ts"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/app/view/web/**/*.ts",
              "src/app/view/web/**/*.html"
            ]
          }
        }
      }
    },
    "mobile": {
      "projectType": "library",
      "root": "src/app/view/mobile",
      "sourceRoot": "src/app/view/mobile",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "src/app/view/mobile/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "tsconfig.build.json"
            },
            "development": {
              "tsConfig": "tsconfig.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "angular.test-setup-file.ts",
            "tsConfig": "./tsconfig.spec.json",
            "karmaConfig": "./karma.conf.js",
            "include": [
              "src/**/*.?(c-|i-)test.ts"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/app/view/mobile/**/*.ts",
              "src/app/view/mobile/**/*.html"
            ]
          }
        }
      }
    },
    "test-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "./test-app",
      "sourceRoot": "./test-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/test-app",
            "index": "test-app/src/index.html",
            "main": "test-app/src/main.ts",
            "polyfills": "test-app/src/polyfills.ts",
            "tsConfig": "test-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "test-app/src/favicon.ico",
              "test-app/src/assets"
            ],
            "styles": [
              "test-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "test-app/src/environments/environment.ts",
                  "with": "test-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "test-app:build:production"
            },
            "development": {
              "browserTarget": "test-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "test-app/src/test.ts",
            "polyfills": "test-app/src/polyfills.ts",
            "tsConfig": "test-app/tsconfig.spec.json",
            "karmaConfig": "test-app/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "test-app/src/favicon.ico",
              "test-app/src/assets"
            ],
            "styles": [
              "test-app/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "test-app/**/*.ts",
              "test-app/**/*.html"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "protractor.conf.js",
            "devServerTarget": "test-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "test-app:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "test-app",
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
